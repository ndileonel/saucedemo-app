{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES6",
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "es6",
      "dom"
    ],
    "types": [
      "cypress",
      "@badeball/cypress-cucumber-preprocessor"
    ],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    "cypress/**/*.ts"
  ]
}